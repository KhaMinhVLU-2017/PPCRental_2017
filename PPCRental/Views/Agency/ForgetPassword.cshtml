
@{
    ViewBag.Title = "ForgetPassword";
    Layout = null;
}


<!DOCTYPE html>
<html>
<head>
    <title>Change Password</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width,initial-scale=1.0">
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/css/font-awesome.css" rel="stylesheet" />
    <link href="~/Content/css/meoStyle.css" rel="stylesheet" />
</head>
<body>
    <div class="container-fluid ">
        <div class="row" style="margin:0;">
            <div class="col-md-offset-4 col-md-4 col-sm-offset-4 col-sm-4 col-xs-12 first_main" id="first">
                <div class="col-md-12 second">
                    <h1>Change Password</h1>
                    <form class="form-group" method="post" action="~/Agency/ForgetPass">
                        <p>Email</p>
                        <input type="text" name="Semail" class="form-control">
                        <p>Phone Number</p>
                        <input type="text" name="number_phone" id="passNew1" class="form-control">
                        <p>@ViewBag.erorr</p>
                        <button class="btn btn-success" type="submit">Change</button>
                        <button class="btn btn-danger">Cancel</button>
                        </p>
                    </form>
                </div>
                <div class="col-md-12 third">
                    <p>Coder by JudasFate</p>
                </div>
            </div>
        </div>
    </div>
    <script src="~/Content/js/msoJS.js"></script>
</body>
</html>
